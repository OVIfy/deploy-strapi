{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "organizer": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "createdby": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "createdevents"
    },
    "start": {
      "type": "datetime"
    },
    "end": {
      "type": "datetime"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "likedby": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "likes"
    },
    "lat": {
      "type": "float"
    },
    "lon": {
      "type": "float"
    },
    "category": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "In-person",
        "Virtual"
      ]
    },
    "location": {
      "type": "string"
    },
    "tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket.ticket",
      "mappedBy": "event"
    },
    "registeredusers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "registeredevents"
    }
  }
}
